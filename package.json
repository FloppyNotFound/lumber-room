{
  "name": "lumber-room",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/FloppyNotFound/lumber-room.git"
  },
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "run-p start:dev build:watch",
    "start": "sirv public",
    "start:dev": "sirv public --dev",
    "validate": "svelte-check",
    "pretty": "prettier --write ./src/**/*.{ts,svelte}",
    "pretty-quick": "pretty-quick --staged",
    "lint": "eslint src --ext .svelte,.ts",
    "lint:style": "stylelint src/**/*.{css,scss,svelte}",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-typescript": "^8.2.1",
    "@tsconfig/svelte": "^1.0.10",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "dropbox": "10.1.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-svelte3": "^3.1.2",
    "husky": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.0.3",
    "pretty-quick": "^3.1.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sass": "^1.32.0",
    "sirv-cli": "^1.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.6.1",
    "tslib": "^2.0.0",
    "typescript": "^4.1.0"
  },
  "dependencies": {
    "dexie": "^3.0.3"
  }
}
