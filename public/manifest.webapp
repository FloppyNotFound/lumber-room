{
  "version": "0.2.0",
  "name": "priviledged-app",
  "description": "Demonstrates how to use permissions to load remote content",
  "launch_path": "/index.html",
  "icons": {
    "16": "/img/icons/icon16x16.png",
    "48": "/img/icons/icon48x48.png",
    "60": "/img/icons/icon60x60.png",
    "128": "/img/icons/icon128x128.png"
  },
  "developer": {
    "name": "Felix Wagner",
    "url": "https://github.com/FloppyNotFound"
  },
  "type": "privileged",
  "permissions": {
    "systemXHR": {
      "description": "Required to load remote content"
    }
  },
  "installs_allowed_from": ["*"],
  "locales": {
    "de": {
      "name": "App mit erweiterten Privilegien",
      "description": "Demonstriert die Verwendung einer KaiOS Anwendung mit erweiterten Privilegien",
      "permissions": {
        "systemXHR": {
          "description": "Ben√∂tigt, um Remote Content zu laden"
        }
      }
    }
  },
  "default_locale": "en"
}
